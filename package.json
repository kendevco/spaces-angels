{
  "name": "@payloadcms/template-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next dev",
    "dev:prod": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" payload generate:types",
    "ii": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next lint",
    "lint:fix": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next lint --fix",
    "payload": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" payload",
    "reinstall": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8192\" next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
